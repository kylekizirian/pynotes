   "source": [
    "## Use ? for non-greedy regex"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "import re"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "equation = \"(x**3 + 3)(x**2 + 2)(x + 1) = 0\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "['(x**3 + 3)(x**2 + 2)(x + 1)']\n"
     ]
    }
   ],
   "source": [
    "print(re.findall(\"\\(.*\\)\", equation))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "['(x**3 + 3)', '(x**2 + 2)', '(x + 1)']\n"
     ]
    }
   ],
   "source": [
    "print(re.findall(\"\\(.*?\\)\", equation))"
   ]
  }
